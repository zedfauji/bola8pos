{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node -e \"(async()=>{try{await require('kill-port')(5173);}catch(e){} process.exit(0)})()\"",
    "dev": "vite --port 5173",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "test:unit": "vitest run --config vite.config.ts",
    "test:integration": "vitest run tests/integration --config vite.config.ts",
    "test:rtl": "vitest run --config vitest.config.ts",
    "test:jest": "jest --config jest.config.cjs",
    "test:jest:watch": "jest --watch --config jest.config.cjs",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:selenium": "jest --config jest.selenium.config.cjs",
    "test:selenium:timeout": "pwsh -NoProfile -File ../../scripts/run-selenium.ps1 -TimeoutSeconds 240 -FrontendPath .",
    "test:selenium:smoke": "jest --config jest.selenium.config.cjs tests/selenium/tests/Smoke.test.ts",
    "test:selenium:smoke:timeout": "pwsh -NoProfile -File ../../scripts/run-selenium.ps1 -TimeoutSeconds 180 -FrontendPath . -JestArgs 'tests/selenium/tests/Smoke.test.ts'",
    "test:selenium:watch": "jest --config jest.selenium.config.cjs --watch",
    "test:selenium:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand --config jest.selenium.config.cjs",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "lint": "eslint .",
    "prepreview": "node -e \"(async()=>{try{await require('kill-port')(5173);}catch(e){} process.exit(0)})()\"",
    "preview": "vite preview --port 5173"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.14.20",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "idb": "^8.0.0",
    "notistack": "^3.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.0.2",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@tanstack/react-query": "^5.85.3",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/chromedriver": "^81.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.0",
    "@types/selenium-webdriver": "^4.1.28",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.5",
    "chromedriver": "^139.0.1",
    "cross-env": "^7.0.3",
    "css.escape": "^1.5.1",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-junit": "^16.0.0",
    "jsdom": "^25.0.1",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.539.0",
    "msw": "^2.6.1",
    "postcss": "^8.5.6",
    "re-resizable": "^6.11.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "selenium-webdriver": "^4.35.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^2.1.9",
    "whatwg-fetch": "^3.6.20"
  }
}
